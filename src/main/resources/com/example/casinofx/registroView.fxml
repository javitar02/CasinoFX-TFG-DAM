            <Font name="JetBrains Mono Regular" size="12.0" />
         </font></TextField>
      <PasswordField fx:id="passwordARegistrar" layoutX="75.0" layoutY="262.0" prefHeight="26.0" prefWidth="150.0" promptText="Contraseña" style="-fx-background-color: red; -fx-text-fill: white;">
