            <Font name="JetBrains Mono Regular" size="36.0" />
         </font>
      </Label>
      <TextField fx:id="nombre" layoutX="75.0" layoutY="92.0" prefHeight="26.0" prefWidth="150.0" promptText="Nombre" style="-fx-background-color: red; -fx-text-fill: white;">
