      <Button layoutX="95.0" layoutY="319.0" mnemonicParsing="false" onAction="#registroUsuario" prefHeight="11.0" prefWidth="110.0" style="-fx-background-color: red; -fx-text-fill: white;" text="REGÍSTRATE" textFill="WHITE">
