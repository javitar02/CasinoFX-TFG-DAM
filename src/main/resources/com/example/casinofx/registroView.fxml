            <Font name="JetBrains Mono Regular" size="12.0" />
         </font></TextField>
      <TextField fx:id="nomUsuARegistrar" layoutX="75.0" layoutY="207.0" prefHeight="26.0" prefWidth="150.0" promptText="Usuario" style="-fx-background-color: red; -fx-text-fill: white;">
